{
  "name": "nkk-www",
  "description": "Make React Project w/ Next.js and Typescript",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Jason Feng <solidzoro@live.com>",
  "private": true,
  "sideEffects": [
    "*.less",
    "*.css"
  ],
  "scripts": {
    "-------------------------------- LINT ----": "----",
    "lint": "next lint",
    "lintall": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "fix": "yarn lint --fix",
    "tsc-check": "tsc -v && tsc --noEmit",
    "format": "prettier --write src/**/*.ts{,x}",
    "prepare": "husky install",
    "-------------------------------- TOOL ----": "----",
    "versionup": "standard-version -r patch",
    "versionup:minor": "standard-version -r minor",
    "versionup:major": "standard-version -r major",
    "versionup:init": "standard-version --first-release",
    "-------------------------------- DEV  ----": "----",
    "dev": "next dev",
    "start": "next start",
    "serve": "yarn build && yarn start",
    "ssg": "next build && next export",
    "export": "next export",
    "build-icons": "react-icons-builder --src ./src/assets/images/icons --dist ./src/libs/react-icons-ext",
    "-------------------------------- PROD ----": "----",
    "build": "next build"
  },
  "dependencies": {
    "antd": "^4.16.13",
    "autoprefixer": "^10.3.7",
    "axios": "^0.22.0",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "classnames": "^2.3.1",
    "css-loader": "^6.3.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.0",
    "moment": "^2.29.1",
    "next": "11.1.2",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.8.1",
    "next-optimized-images": "^2.6.2",
    "next-plugin-antd-less": "^1.4.3",
    "normalize.css": "^8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet-async": "^1.1.2",
    "react-icons": "^4.3.1",
    "react-router-dom": "^5.3.0",
    "react-use": "^17.3.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^13.2.0",
    "@types/classnames": "^2.3.1",
    "@types/eslint": "^7.28.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "^16.10.2",
    "@types/prettier": "^2.4.1",
    "@types/react": "^17.0.27",
    "@types/react-router-dom": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "commitlint": "^13.2.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-qcolate": "^1.2.7",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.5.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "git-repo-info": "^2.1.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.0",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "react-icons-builder": "1.0.1",
    "typescript": "^4.4.3"
  }
}
